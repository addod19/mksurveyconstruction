
<tabel>
    <tr>
        <th>Customer Name</th>
        <th>Customer Email</th>
        <th>Phone Number</th>
        <th>Date Created</th>
        <th>Date Updated</th>
    <tr>
    <tr>
        <% @users.each do |user| %>
            <td>user.name</td>
            <td>user.email</td>
            <td>user.phone_number</td>
            <td>user.created_at</td>
            <td>user.updated_at</td>
        <% end %>
    </tr>
</table>
